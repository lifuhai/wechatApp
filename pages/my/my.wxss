/* pages/my/my.wxss */
.mylayout {
  height: 35vh;
}
.mylayout .user_image_wrap .user_bg {
  height: 35vh;
  filter: blur(3rpx);
}
.mylayout .user_image_wrap .avater_and_name {
  position: absolute;
  left: 50%;
  top: 10%;
  transform: translateX(-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.mylayout .user_image_wrap .avater_and_name image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.mylayout .user_image_wrap .avater_and_name button {
  margin-top: 10rpx;
  color: white;
  font-size: 35rpx;
  background-color: #00000000;
}
.myorder {
  width: 100%;
  height: 90rpx;
  background-color: white;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  border-bottom: 1rpx solid #f3f3f3;
  font-size: 35rpx;
}
.orderInfo {
  display: flex;
  border-bottom: 15rpx solid #f3f3f3;
}
.orderInfo .orderNo {
  display: flex;
  flex-direction: column;
  flex: 1;
  padding: 15rpx;
  align-items: center;
}
.orderInfo .orderNo image {
  width: 40%;
}
.orderInfo .orderNo text {
  margin-top: 10rpx;
}
.indexMeau {
  display: flex;
  flex-wrap: wrap;
}
.indexMeau .meau {
  width: 25%;
}
.indexMeau .meau navigator {
  width: 100%;
  height: 187.5rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
  align-items: center;
  justify-content: center;
  border: 0rpx solid #f5f5f5;
}
.indexMeau .meau navigator image {
  width: 30%;
}
.indexMeau .meau navigator text {
  margin-top: 10rpx;
}
